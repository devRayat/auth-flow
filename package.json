{
  "name": "generate",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node --register dotenv/config build/cjs/index.cjs.js",
    "build": "rollup --config rollup.config.js --environment NODE_ENV:production",
    "build:dev": "rollup --config rollup.config.js --environment NODE_ENV:development",
    "watch": "rollup --config rollup.config.js --watch",
    "dev": "yarn watch --environment NODE_ENV:development",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js",
    "sync": "yarn typeorm schema:sync"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^14.1.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.7.1",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-run": "^2.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.9",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "rollup": "^2.64.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-tsconfig-paths": "^1.1.6",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
